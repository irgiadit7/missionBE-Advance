generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Course {
  id          Int     @id @default(autoincrement())
  title       String 
  description String
  price       Int
  author      String  
  category    String 
  imageUrl    String  
  rating      Float  
}